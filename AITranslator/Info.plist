<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>AITranslatorCloudEndpoint</key>
	<string>$(AITRANSLATOR_CLOUD_ENDPOINT)</string>
	<key>AITranslatorCloudSecret</key>
	<string>$(AITRANSLATOR_CLOUD_SECRET)</string>
	<key>com.apple.developer.translation-ui-provider.network-access</key>
	<true/>
	<key>NSServices</key>
	<array>
		<dict>
			<key>NSMenuItem</key>
			<dict>
				<key>default</key>
				<string>Translate with TLingo</string>
			</dict>
			<key>NSMessage</key>
			<string>translateText</string>
			<key>NSPortName</key>
			<string>TLingo</string>
			<key>NSSendTypes</key>
			<array>
				<string>public.utf8-plain-text</string>
				<string>NSStringPboardType</string>
			</array>
		</dict>
	</array>
</dict>
</plist>
